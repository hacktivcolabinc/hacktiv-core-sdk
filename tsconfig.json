{
  "compilerOptions": {
    "incremental": true,
    "outDir": "dist",
    "rootDir": "src",
    "declaration": true,
    "module": "CommonJS",
    "target": "ES5",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "lib": ["ES2017", "DOM"]
  },
  "include": [
    "src/**/*"
  ],
}